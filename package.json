{
  "author": {
    "name": "Ivo Janssen",
    "email": "hello@ivo.la"
  },
  "scripts": {
    "lint": "eslint ."
  },
  "contributors": [
    {
      "name": "emersion"
    }
  ],
  "bugs": {
    "url": "https://github.com/foxxyz/node-clementine-client/issues"
  },
  "dependencies": {
    "protobufjs": "^6.10.1"
  },
  "description": "A Node.js library for Clementine remote protocol",
  "devDependencies": {
    "eslint": "^7.8.1"
  },
  "homepage": "https://github.com/foxxyz/node-clementine-client",
  "keywords": [
    "clementine",
    "remote",
    "music",
    "player",
    "protocol"
  ],
  "license": "MIT",
  "main": "index.js",
  "name": "clementine-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxxyz/node-clementine-client.git"
  },
  "version": "1.0.0",
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module"
    },
    "rules": {
      "eqeqeq": [
        "error",
        "always"
      ],
      "indent": [
        "error",
        4
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "info",
            "error"
          ]
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "prefer-const": [
        "error"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
